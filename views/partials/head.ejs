    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
    <title><%= title %></title>
    <meta name="description" content="<%= description %>">
    <link rel="canonical" href="<%= canonical %>">
    
    <meta property="og:title" content="<%= title %>">
